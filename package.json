    "react-bootstrap-table-next": "^0.1.9",
    "react-bootstrap-table2-editor": "^0.2.0",
    "react-chessdiagram": "^1.3.3",
    "react-dom": "^16.3.2",
    "react-media": "^1.8.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-table": "^6.8.2",
